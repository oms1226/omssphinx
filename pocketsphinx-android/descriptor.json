{
  "package": {
    "subject": "edu.cmu.pocketsphinx",
    "repo": "android",
    "name": "pocketsphinx-android",
    "vcs_url": "https://github.com/cmusphinx/pocketsphinx-android.git",
    "licenses": ["BSD 3-Clause"],
